---
const { education } = Astro.props;
---

<section>
  <h2>Education</h2>
  {education.items.map((item: { area: string; institution: string; studyType: string; date: string; }) => (
    <div>
      <h3>{item.area}</h3>
      <p>{item.institution} | {item.studyType}</p>
      <p>{item.date}</p>
    </div>
  ))}
</section>